@model AcadSchedule.WebUI.Models.SubjectsListViewModel

@{
    ViewBag.Title = "Lista przedmiotów";
}

@foreach (var s in Model.Subjects)
{
    Html.RenderPartial("SubjectsPartialView", s);
}

<div class="pager">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("SubjectList", new { page = x }))
</div>
